<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Music</title>
    <script src="https://kit.fontawesome.com/d41001deaf.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="/stylesheets/styles.css">
</head>
<body class="music">
  <header class="row">
    <nav class="menu">
      <ul>
        <li><a href="/">home</a></li>
        <li><a href="/about">about</a></li>
        <li><a href="/music">music</a></li>
        <li><a href="/contact">contact</a></li>
      </ul>
  </nav>
  <nav class="redes">
    <a href="" class="twitter"><i class="fab fa-twitter"></i></a>
    <a href="" class="facebook"><i class="fab fa-facebook-f"></i></a>
    <a href="" class="spotify"><i class="fab fa-spotify"></i></a>
    <a href="" class="itunes"><i class="fab fa-itunes"></i></a>
    <a href="" class="amazon"><i class="fab fa-amazon"></i></a>
    <a href="" class="instagram"><i class="fab fa-instagram"></i></a>
  </nav>
</header>
    <main>
      <h1>MUSIC//</h1>
      <p>Direct links</p>
      <ul>
        <li><a href="">Album</a></li>
        <li><a href="">Video: Organic Love</a></li>
        <li><a href="">Video: Street Light</a></li>
      </ul>
      <section>
        <article id="album">
          <h3>ALBUM<br> TWIST IN MY REALITY</h3>
          <img src="/images/emilia-play-list.png" alt="playlist">
          <ol>
            <li><a href=""><i class="fas fa-play"></i>Organic Love | 02:40</a></li>
            <li><a href=""><i class="fas fa-play"></i>Twist In My Reality | 02:10</a></li>
            <li><a href=""><i class="fas fa-play"></i>Street Light | 03:28</a></li>
            <li><a href=""><i class="fas fa-play"></i>Love Donâ€™t Live Here Anymore (Cover) | 03:49</a></li>
          </ol>
          <a class="go" href="#">Go Top</a>
        </article>

        <% videos.forEach(video => { %>
          <article id=<%= video.ancla %> >
            <h3>VIDEO <br><%= video.title %> </h3>
            <iframe src=<%= video.link  %>  frameborder="0"
          allow="autoplay; fullscreen" allowfullscreen></iframe>
          <p><%= video.director %> <br><%= video.editor %> <br>Produced by <%= video.productor %></p>
            <a class="go" href="#">Go Top</a>
          </article>
         
        <% }) %>
      </section>
    </main>
</body>
</html>